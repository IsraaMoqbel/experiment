<link rel="stylesheet" href="/css/styleSignUp.css">
<!-- loginnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnn -->
 <div id="login">
  <h1>Welcome Back!</h1>

  <form action="/login" method="post">

    <div class="field-wrap">
      <!-- <label>
          Email Address<span class="req">*</span>
        </label> -->
        <input type="email" name="email" required autocomplete="off" placeholder="Enter your Email"/>

    </div>
    <div class="field-wrap">
      <!-- <label>
          Password<span class="req">*</span>
        </label> -->
      <input type="password" name="password" required autocomplete="off" placeholder="Password"/>
    </div>

    <p class="forgot"><a href="#">Forgot Password?</a></p>

    <button class="button button-block" />Log In</button>

  </form>

</div>
